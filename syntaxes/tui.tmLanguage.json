{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Tui",
  "patterns": [{ "include": "#comment" }],
  "repository": {
    "comment": {
      "patterns": [
        {
          "name": "comment.line.double-slash.tui",
          "begin": "//",
          "end": "$"
        },
        {
          "name": "comment.line.number-sign.tui",
          "begin": "#",
          "end": "$"
        }
      ]
    }
  },
  "scopeName": "source.tui"
}
